; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="fractiwi"
run/main_scene="res://main/main.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

Ref="*res://main/ref.gd"
Data="*res://main/data/data.gd"

[debug]

gdscript/warnings/redundant_await=0

[display]

window/size/viewport_width=64
window/size/viewport_height=64
window/size/window_width_override=512
window/size/window_height_override=512
window/stretch/mode="viewport"

[file_customization]

folder_colors={
"res://assets/": "gray",
"res://main/battle/": "yellow",
"res://main/battle/action/battle_animation/": "teal",
"res://main/battle/action/icons/": "teal",
"res://main/battle/fighter/effect/": "teal",
"res://main/battle/ui/": "orange",
"res://main/music/": "green",
"res://main/world/": "blue",
"res://main/world/ui/": "orange"
}

[importer_defaults]

oggvorbisstr={
"bar_beats": 4,
"beat_count": 0,
"bpm": 0,
"loop": true,
"loop_offset": 0
}

